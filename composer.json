{
    "name": "shel/blog",
    "type": "neos-plugin",
    "license": "MIT",
    "description": "A simple blog for Neos CMS based on fusion and yaml configurations. Also supports atom feeds.",
    "require": {
        "neos/neos": "~3.0",
        "neos/nodetypes": "~3.0"
    },
    "authors": [
        {
            "name": "Sebastian Helzle",
            "email": "sebastian@helzle.net",
            "homepage": "http://www.sebastianhelzle.net",
            "role": "Developer"
        }
    ],
    "suggest": {
        "typo3/twitter-bootstrap": "The provided templates use bootstrap classes. These can be changed or you can just use the bootstrap package."
    },
    "autoload": {
        "psr-0": {
            "Shel\\Blog": "Classes"
        }
    },
    "extra": {
        "applied-flow-migrations": [
            "TYPO3.FLOW3-201201261636",
            "TYPO3.Fluid-201205031303",
            "TYPO3.FLOW3-201205292145",
            "TYPO3.FLOW3-201206271128",
            "TYPO3.FLOW3-201209201112",
            "TYPO3.Flow-201209251426",
            "TYPO3.Flow-201211151101",
            "TYPO3.Flow-201212051340",
            "TYPO3.TypoScript-130516234520",
            "TYPO3.TypoScript-130516235550",
            "TYPO3.TYPO3CR-130523180140",
            "TYPO3.Neos.NodeTypes-201309111655",
            "TYPO3.Flow-201310031523",
            "TYPO3.Flow-201405111147",
            "TYPO3.Neos-201407061038"
        ]
    }
}